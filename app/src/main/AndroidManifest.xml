<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.handou.activitystackstest">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.handou.activitystackstest.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.handou.activitystackstest.ActivityStandard"
            android:launchMode="standard" />
        <activity
            android:name="com.example.handou.activitystackstest.ActivitySingleTop"
            android:launchMode="singleTop"/>
        <activity
            android:name="com.example.handou.activitystackstest.ActivitySingleTask"
            android:launchMode="singleTask"/>
        <activity
            android:name="com.example.handou.activitystackstest.ActivitySingleInstance"
            android:launchMode="singleInstance"/>
    </application>

</manifest>
